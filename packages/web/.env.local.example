# Google AI API Key (required for AI explanations)
GOOGLE_AI_API_KEY=your_api_key_here

# Path to the repository being analyzed (optional, defaults to process.cwd())
REPO_PATH=

# WebSocket URL for live topology updates (optional)
# In Docker, this is auto-detected from window.location.hostname
# Override for TLS/reverse proxy setups, e.g.: wss://topology.example.com/ws
NEXT_PUBLIC_WS_URL=
