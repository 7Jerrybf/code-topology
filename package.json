{
  "name": "code-topology-mvp",
  "version": "0.1.0",
  "private": true,
  "description": "Local-first visualization tool for code dependency analysis",
  "scripts": {
    "analyze": "npx tsx cli/src/index.ts analyze .",
    "cli": "pnpm --filter @topology/cli",
    "web": "pnpm --filter @topology/web",
    "dev:web": "turbo dev --filter=@topology/web",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck"
  },
  "keywords": [
    "ast",
    "topology",
    "visualization",
    "code-analysis"
  ],
  "license": "MIT",
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "turbo": "^2.8.3"
  }
}